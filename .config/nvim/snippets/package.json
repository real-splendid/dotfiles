{
  "name": "my-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdow",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./global.json"
      },
      {
        "language": [
          "css",
          "scs",
          "sass",
          "less",
          "stylus"
        ],
        "path": "./css.json"
      },
      {
        "language": "dockerfile",
        "path": "./docker_file.json"
      },
      {
        "language": "yaml",
        "path": "./docker-compose.json"
      },
      {
        "language": "go",
        "path": "./go.json"
      },
      {
        "language": "html",
        "path": "./html.json"
      },
      {
        "language": "javascript",
        "path": "./javascript.json"
      },
      {
        "language": "lua",
        "path": "./lua.json"
      },
      {
        "language": "php",
        "path": "./php.json"
      }
    ]
  }
}
